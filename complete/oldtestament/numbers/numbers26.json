{
    "_id": "numbers26",
    "nextChapterId": "numbers27",
    "prevChapterId": "numbers25",
    "volume": {
        "title": "Old Testament",
        "titleShort": "OT",
        "titleOfficial": "The Old Testament",
        "_id": "oldtestament"
    },
    "book": {
        "title": "Numbers",
        "titleShort": "Num",
