{
    "_id": "exodus30",
    "nextChapterId": "exodus31",
    "prevChapterId": "exodus29",
    "volume": {
        "title": "Old Testament",
        "titleShort": "OT",
        "titleOfficial": "The Old Testament",
        "_id": "oldtestament"
    },
    "book": {
        "title": "Exodus",
        "titleShort": "Ex",
