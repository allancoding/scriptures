{
    "_id": "luke2",
    "nextChapterId": "luke3",
    "prevChapterId": "luke1",
    "volume": {
        "title": "New Testament",
        "titleShort": "NT",
        "titleOfficial": "The New Testament",
        "_id": "newtestament"
