{
    "_id": "luke1",
    "nextChapterId": "luke2",
    "prevChapterId": "mark16",
    "volume": {
        "title": "New Testament",
        "titleShort": "NT",
        "titleOfficial": "The New Testament",
        "_id": "newtestament"
    },
    "book": {
        "title": "Luke",
        "titleShort": "Luke",
