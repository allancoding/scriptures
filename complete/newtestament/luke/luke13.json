{
    "_id": "luke13",
    "nextChapterId": "luke14",
    "prevChapterId": "luke12",
    "volume": {
        "title": "New Testament",
        "titleShort": "NT",
        "titleOfficial": "The New Testament",
        "_id": "newtestament"
    },
    "book": {
        "title": "Luke",
        "titleShort": "Luke",
