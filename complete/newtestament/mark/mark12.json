{
    "_id": "mark12",
    "nextChapterId": "mark13",
    "prevChapterId": "mark11",
    "volume": {
        "title": "New Testament",
        "titleShort": "NT",
        "titleOfficial": "The New Testament",
        "_id": "newtestament"
    },
    "book": {
        "title": "Mark",
        "titleShort": "Mark",
