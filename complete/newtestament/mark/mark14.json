{
    "_id": "mark14",
    "nextChapterId": "mark15",
    "prevChapterId": "mark13",
    "volume": {
        "title": "New Testament",
        "titleShort": "NT",
        "titleOfficial": "The New Testament",
        "_id": "newtestament"
    },
    "book": {
        "title": "Mark",
        "titleShort": "Mark",
        "titleOfficial": "",
