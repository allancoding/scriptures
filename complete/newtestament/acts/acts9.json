{
    "_id": "acts9",
    "nextChapterId": "acts10",
    "prevChapterId": "acts8",
    "volume": {
        "title": "New Testament",
        "titleShort": "NT",
        "titleOfficial": "The New Testament",
        "_id": "newtestament"
    },
