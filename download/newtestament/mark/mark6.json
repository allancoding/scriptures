{
    "_id": "mark6",
    "nextChapterId": "mark7",
    "prevChapterId": "mark5",
    "volume": {
        "title": "New Testament",
        "titleShort": "NT",
        "titleOfficial": "The New Testament",
        "_id": "newtestament"
    },
    "book": {
        "title": "Mark",
        "titleShort": "Mark",
        "titleOfficial": "",
